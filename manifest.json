{
  "manifest_version": 3,
  "name": "Workout Focus Mode",
  "description": "Hide everything except the workout and show it full screen.",
  "version": "1.0",
  "permissions": ["scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_title": "Toggle Workout Focus Mode"
  },
  "background": {
    "service_worker": "background.js"
  }
}
